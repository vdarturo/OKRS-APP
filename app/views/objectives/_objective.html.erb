<div id="<%= dom_id objective %>">
  <div class="row">
    <div class="col-6">
      <%= label_tag "Name" %>
      <%= text_field_tag "name", objective.name, class:"form-control w-100", readonly: true %>
    </div>
    <div class="col-6">
      <%= label_tag "Owner" %>
      <%= text_field_tag "name", @owner.name, class:"form-control w-100", readonly: true %>
    </div>
    <div class="col-12">      
      <%= label_tag "Description" %>      
      <%= text_area_tag "description", objective.description, class:"form-control w-100", readonly: true %>
    </div>    
  </div>
</div>